{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Start Node.js API",
			"type": "shell",
			"command": "node",
			"args": [
				"server.js"
			],
			"options": {
				"cwd": "${workspaceFolder}/nodejsAPI"
			},
			"group": "build",
			"isBackground": true,
			"problemMatcher": [],
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": true,
				"clear": false
			}
		}
	]
}